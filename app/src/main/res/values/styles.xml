<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
<style name="Numbers">
 <item name="android:textColor">#FFFFFFFF</item>
 <item name="android:textSize">25dp</item>
 <item name="android:background">@color/numbers_background</item>
</style>
    <style name="FamilyMember">
     <item name="android:background">@color/family_background</item>
     <item name="android:textSize">25dp</item>
     <item name="android:textColor">#FFFFFFFF</item>
    </style>
    <style name="Colors">
     <item name="android:background">@color/Colors_background</item>
     <item name="android:textColor">#FFFFFFFF</item>
      <item name="android:textSize">25dp</item>

    </style>
    <style name="phrases">
     <item name="android:textColor">#FFFFFFFF</item>
     <item name="android:background">@color/phrase_background</item>
      <item name="android:textSize">25dp</item>
    </style>



</resources>
